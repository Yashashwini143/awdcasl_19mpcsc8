import java.util.*;
  
public class collection_min_max {
    public static void main(String args[])
    {
        Set<Integer> set = new HashSet<Integer>();
        set.add(10);
        set.add(20);
        set.add(5);
        set.add(40);
        set.add(7);
        System.out.println("Set: " + set);
        int minSet = Collections.min(set);
        int maxSet = Collections.max(set);
        System.out.println("Maximum value of set is: "
                           + maxSet);
        System.out.println("Minimum value of set is: "
                           + minSet);
    }
}
output:
Set: [20, 5, 7, 40, 10]
Maximum value of set is: 40
Minimum value of set is: 5
